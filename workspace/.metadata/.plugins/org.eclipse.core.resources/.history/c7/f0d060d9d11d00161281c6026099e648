public class Product {
String name;
String description;
int quantityInStock;
double weight;
double price;











public String getName() {
	return name;
}
public String getDescription() {
	return description;
}
public int getQuantityInStock() {
	return quantityInStock;
}
public double getWeight() {
	return weight;
}
public double getPrice() {
	return price;
}

}
